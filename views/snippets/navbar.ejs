<div id="deadbirdNav">
  <ul>
    <%
      if (originalUrl.match('leaderboards') || originalUrl.match('stats')) {
    %>
        <li><a href="/">View Stream</a></li>&nbsp;|&nbsp;
    <%
      }

      if (originalUrl === '/' || originalUrl.match('page=') || originalUrl.match('stats')) {
    %>
        <li><a href="/leaderboards">View Leaderboards</a></li><%-originalUrl.match('stats') ? "<br>" : "&nbsp;|&nbsp;"%>
    <%
      }

      if (originalUrl === '/' || originalUrl.match('page=') || originalUrl.match('leaderboards')) {
    %>
        <li><a href="/stats">View Stats</a></li><br>
    <%
      }
    %>
    <br>
    <%
      if (originalUrl.match('stats')) {
    %>
        <li><input id="deadbirdSearch" type='text' placeholder='View specific user stats' autofocus></li>
    <%
      } else {
    %>
      <li><input id="deadbirdSearch" type='text' placeholder='View/Add Twitter User' autofocus></li>
    <%
      }
    %>
  </ul>
</div>
